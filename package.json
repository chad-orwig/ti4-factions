{
  "name": "ti4-factions",
  "version": "1.0.0",
  "description": "A small application for viewing factions in Twilight Imperium 4",
  "main": "src/server/devServer.js",
  "repository": "https://github.com/chad-orwig/ti4-factions",
  "scripts": {
    "dev": "ENV=local npm-run-all --parallel dev:server lint:watch",
    "lint": "esw src --ext .js,.jsx",
    "lint:watch": "npm run lint -- --watch",
    "dev:server": "node src/server/devServer.js",
    "clean-dist": "npm run remove-dist && mkdir src/server/dist",
    "remove-dist": "rimraf ./src/server/dist",
    "build:html": "node src/server/buildHtml.js",
    "prebuild": "npm-run-all lint clean-dist build:html",
    "build": "node src/server/build.js",
    "predeploy": "npm run build",
    "deploy": "gcloud app deploy --quiet --project ti4-factions",
    "start": "npm run dist:server",
    "dist:server": "node src/server/distServer.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.3",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "file-loader": "^1.1.11",
    "font-awesome-sass": "^4.7.0",
    "jsx-loader": "^0.13.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.7.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.29.6",
    "webpack-dev-middleware": "^3.6.1"
  },
  "devDependencies": {
    "acorn": "^6.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-watch": "^5.0.1",
    "webpack-hot-middleware": "^2.24.3"
  }
}
